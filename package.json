{
  "name": "symphony-dashboard",
  "version": "1.0.0",
  "description": "D3.js dashboard for symphony ticket sales analytics",
  "main": "index.html",
  "scripts": {
    "dev": "netlify dev",
    "build": "echo 'Static build complete'",
    "start": "netlify dev",
    "create-tables": "node scripts/archive/create-bigquery-tables.js",
    "migrate-to-bigquery": "node scripts/archive/migrate-to-bigquery-bulk.js",
    "test-bigquery": "node scripts/tests/test-bigquery.js",
    "bq-setup": "echo 'Follow bigquery-setup-instructions.md for BigQuery setup'",
    "apply-schema-enhancements": "node scripts/archive/apply-schema-enhancements.js",
    "test-webhook": "node scripts/tests/test-webhook.js",
    "test-makecom": "node scripts/tests/test-webhook.js --makecom",
    "extract-excel": "node scripts/archive/extract-excel-data.js",
    "extract-bigquery": "node scripts/active/extract-bigquery-data.js",
    "merge-data": "node scripts/archive/merge-excel-bigquery.js",
    "build-dashboard-data": "node scripts/active/create-dashboard-data.js",
    "refresh-dashboard": "npm run extract-excel && npm run build-dashboard-data",
    "fix-subscription-data": "node scripts/archive/fix-subscription-data.js",
    "backfill-dry-run": "node scripts/active/process-pdf-bucket.js gs://symphony-dashboard-pdfs/2025/10 --dry-run",
    "backfill-historical": "node scripts/active/process-pdf-bucket.js gs://symphony-dashboard-pdfs/2025/10",
    "validate-backfill": "node scripts/diagnostic/validate-backfill.js",
    "feature:start": "git checkout develop && git pull origin develop && git checkout -b",
    "deploy:production": "git checkout main && git merge develop && git push origin main",
    "deploy:preview": "git push origin HEAD"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^8.1.1",
    "@google-cloud/storage": "^7.17.2",
    "canvas": "^3.2.0",
    "csv-parse": "^6.1.0",
    "d3": "^7.8.5",
    "dotenv": "^17.2.2",
    "jsonwebtoken": "^9.0.2",
    "pdf-parse": "^1.1.1",
    "pdf-to-text": "^0.0.7",
    "pdf2json": "^3.2.2",
    "pdfjs-dist": "^5.4.149",
    "undici": "^7.16.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "netlify-cli": "^17.0.0"
  },
  "keywords": [
    "d3",
    "dashboard",
    "symphony",
    "analytics",
    "tessitura"
  ],
  "engines": {
    "node": ">=20.18.0"
  },
  "author": "",
  "license": "MIT"
}
