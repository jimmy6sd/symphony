# Symphony Dashboard Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control!

# Tessitura API Configuration (Server-side only for data fetching)
TESSITURA_BASE_URL=https://KFFCTRUSMO0webprod.tnhs.cloud/tessitura/api
TESSITURA_USERNAME=jhender
TESSITURA_PASSWORD=ForTheShowKCSY25!
TESSITURA_USER_GROUP=syweb
TESSITURA_MACHINE_LOCATION=jhender

# Dashboard Authentication (Simple login for users to view dashboard)
DASHBOARD_AUTH_USERNAME=symphonyadmin
DASHBOARD_AUTH_PASSWORD=SymphonyDash2025!

# JWT Security (CHANGE THESE IN PRODUCTION!)
JWT_SECRET=your-super-secure-jwt-secret-key-here-minimum-32-characters
JWT_EXPIRY=24h

# Application Settings
NODE_ENV=production
PORT=3000

# Google Cloud BigQuery Configuration
GOOGLE_CLOUD_PROJECT_ID=kcsymphony
BIGQUERY_DATASET=symphony_dashboard

# For local development - path to service account key file
GOOGLE_APPLICATION_CREDENTIALS=./symphony-bigquery-key.json

# For production (Netlify) - paste the entire JSON content as one line
# GOOGLE_APPLICATION_CREDENTIALS_JSON={"type":"service_account","project_id":"kcsymphony",...}

# API Rate Limiting
TESSITURA_RATE_LIMIT=100
TESSITURA_RATE_WINDOW=60000

# Cache Settings
CACHE_DURATION=300000
ENABLE_CACHE=true

# Development Settings
LOG_LEVEL=info