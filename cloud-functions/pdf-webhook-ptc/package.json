{
  "name": "symphony-ptc-webhook",
  "version": "1.0.0",
  "description": "Google Cloud Function for processing PTC (Price Type Category) PDFs - Comp ticket extraction",
  "main": "index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.0.0",
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/storage": "^7.0.0",
    "pdf2json": "^3.0.5"
  },
  "scripts": {
    "start": "functions-framework --target=ptcPdfWebhook --signature-type=http",
    "deploy": "gcloud functions deploy symphony-ptc-webhook --gen2 --runtime=nodejs20 --region=us-central1 --source=. --entry-point=ptcPdfWebhook --trigger-http --allow-unauthenticated --memory=1GB --timeout=300s",
    "logs": "gcloud functions logs read symphony-ptc-webhook --region=us-central1 --gen2 --limit=50"
  }
}
